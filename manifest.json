{
    "$schema": "https://raw.githubusercontent.com/FLECS-Technologies/app-sdk/main/manifest.schema.json",
    "_schemaVersion": "2.0.0",
    "app": "com.somic-packaging.labrador",
    "version": "##VERSION##",
    "revision": "0",
    "image": "flecs.azurecr.io/com.somic-packaging.labrador",
    "multiInstance": false,
    "editor": ":9080",
    "ports": [
        "9080:8081"
    ],
    "volumes": [
        "/home/somic/app/labrador/:/labrador"
    ],
    "env": [
        "ApplicationSettingsPath=/labrador/labrador.yaml",
        "ConnectionStrings__SQLiteDefault=DataSource=/labrador/labrador.db",
        "LabradorLocation=/labrador/",
        "FRONTEND_PORT=5173",
        "BACKEND_PORT=5222",
        "SERVER_IP=0.0.0.0"
    ]
}
